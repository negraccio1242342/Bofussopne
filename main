-- Core script hosted on GitHub
local Script = getgenv().Script
if not Script or not Script.Teleport or not Script.Teleport.Enabled then return end

local UserInputService = game:GetService("UserInputService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()

-- Teleport function
local function teleportToRandomPosition()
    if not char or not char:FindFirstChild("HumanoidRootPart") then return end
    local root = char.HumanoidRootPart

    -- Change these ranges as needed
    local newPosition = root.Position + Vector3.new(
        math.random(-50, 50),
        math.random(5, 25),
        math.random(-50, 50)
    )

    root.CFrame = CFrame.new(newPosition)
end

-- Key listener
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Script.Teleport.Keybind then
        teleportToRandomPosition()
    end
end)
